{
  "contributions": {
    "settings": [
      {
        "file_patterns": [
          "/LSP-sass.sublime-settings"
        ],
        "schema": {
          "$id": "sublime://settings/LSP-sass",
          "definitions": {
            "PluginConfig": {
              "properties": {
                "settings": {
                  "additionalProperties": false,
                  "properties": {
                    "somesass.workspace.exclude": {
                      "type": "array",
                      "items": {
                        "type": "string"
                      },
                      "default": [
                        "**/.git/**",
                        "**/node_modules/**"
                      ],
                      "description": "List of glob patterns for directories that are excluded in the initial scan for Sass files. Files in the exclude list will still be processed if referenced by @use, @forward and @import (for example a depencendy you use from node_modules)."
                    },
                    "somesass.workspace.loadPaths": {
                      "type": "array",
                      "items": {
                        "type": "string"
                      },
                      "default": [],
                      "description": "A list of paths relative to the workspace root where the language server should look for stylesheets loaded by @use and @import. node_modules is always included. Note that you will have to configure your Sass compiler separately."
                    },
                    "somesass.workspace.logLevel": {
                      "type": "string",
                      "default": "info",
                      "enum": [
                        "fatal",
                        "error",
                        "warn",
                        "info",
                        "debug",
                        "trace"
                      ],
                      "description": "Control how much gets logged to the Output window."
                    },
                    "somesass.scss.codeAction.enabled": {
                      "type": "boolean",
                      "default": true,
                      "description": "Enable or disable all code actions."
                    },
                    "somesass.scss.colors.enabled": {
                      "type": "boolean",
                      "default": true,
                      "description": "Enable or disable all color decorators."
                    },
                    "somesass.scss.colors.includeFromCurrentDocument": {
                      "type": "boolean",
                      "default": true,
                      "description": "Compatibility setting for VS Code. By default the built-in SCSS server shows color decorators for variables declared in the current document. To avoid duplicates Some Sass will not show them unless you opt in."
                    },
                    "somesass.scss.completion.enabled": {
                      "type": "boolean",
                      "default": true,
                      "description": "Enable or disable all completions (IntelliSense)."
                    },
                    "somesass.scss.completion.css": {
                      "type": "boolean",
                      "default": true,
                      "description": "Include CSS completions."
                    },
                    "somesass.scss.completion.includeFromCurrentDocument": {
                      "type": "boolean",
                      "default": true,
                      "description": "Compatibility setting for VS Code. By default the built-in SCSS server shows suggestions for variables, mixins and functions declared in the current document. To avoid duplicates Some Sass will not suggest them unless you opt in."
                    },
                    "somesass.scss.completion.suggestFromUseOnly": {
                      "type": "boolean",
                      "default": false,
                      "description": "If your project uses the new module system with @use and @forward, you may want to only include suggestions from your used modules."
                    },
                    "somesass.scss.completion.mixinStyle": {
                      "type": "string",
                      "default": "all",
                      "enum": [
                        "all",
                        "nobracket",
                        "bracket"
                      ],
                      "enumItemLabels": [
                        "All",
                        "No brackets",
                        "Only brackets"
                      ],
                      "enumDescriptions": [
                        "Show all suggestions",
                        "Only show suggestions without brackets",
                        "Where brackets are suggested, don't suggest without brackets"
                      ],
                      "description": "Controls the style of suggestions for mixins."
                    },
                    "somesass.scss.completion.triggerPropertyValueCompletion": {
                      "type": "boolean",
                      "default": true,
                      "description": "By default, Some Sass triggers property value completion after selecting a CSS property. Use this setting to disable this behavior."
                    },
                    "somesass.scss.completion.completePropertyWithSemicolon": {
                      "type": "boolean",
                      "default": true,
                      "description": "Insert semicolon at end of line when completing CSS properties."
                    },
                    "somesass.scss.definition.enabled": {
                      "type": "boolean",
                      "default": true,
                      "description": "Enable or disable Go to Definition."
                    },
                    "somesass.scss.diagnostics.enabled": {
                      "type": "boolean",
                      "default": true,
                      "description": "Enable or disable all diagnostics (deprecation, errors and lint rules)."
                    },
                    "somesass.scss.diagnostics.deprecation.enabled": {
                      "type": "boolean",
                      "default": true,
                      "description": "Enable or disable deprecation diagnostics (strike-through)."
                    },
                    "somesass.scss.diagnostics.lint.enabled": {
                      "type": "boolean",
                      "default": true,
                      "description": "Enable or disable all linting."
                    },
                    "somesass.scss.diagnostics.lint.compatibleVendorPrefixes": {
                      "default": "ignore",
                      "description": "When using a vendor-specific prefix make sure to also include all other vendor-specific properties.",
                      "enum": ["ignore", "warning", "error"],
                      "type": "string"
                    },
                    "somesass.scss.diagnostics.lint.vendorPrefix": {
                      "default": "warning",
                      "description": "When using a vendor-specific prefix, also include the standard property.",
                      "enum": ["ignore", "warning", "error"],
                      "type": "string"
                    },
                    "somesass.scss.diagnostics.lint.duplicateProperties": {
                      "default": "ignore",
                      "description": "Do not use duplicate style definitions.",
                      "enum": ["ignore", "warning", "error"],
                      "type": "string"
                    },
                    "somesass.scss.diagnostics.lint.emptyRules": {
                      "default": "warning",
                      "description": "Do not use empty rulesets.",
                      "enum": ["ignore", "warning", "error"],
                      "type": "string"
                    },
                    "somesass.scss.diagnostics.lint.importStatement": {
                      "default": "ignore",
                      "description": "Import statements do not load in parallel.",
                      "enum": ["ignore", "warning", "error"],
                      "type": "string"
                    },
                    "somesass.scss.diagnostics.lint.boxModel": {
                      "default": "ignore",
                      "enum": ["ignore", "warning", "error"],
                      "markdownDescription": "Do not use `width` or `height` when using `padding` or `border`.",
                      "type": "string"
                    },
                    "somesass.scss.diagnostics.lint.universalSelector": {
                      "default": "ignore",
                      "enum": ["ignore", "warning", "error"],
                      "markdownDescription": "The universal selector (`*`) is known to be slow.",
                      "type": "string"
                    },
                    "somesass.scss.diagnostics.lint.zeroUnits": {
                      "default": "ignore",
                      "description": "No unit for zero needed.",
                      "enum": ["ignore", "warning", "error"],
                      "type": "string"
                    },
                    "somesass.scss.diagnostics.lint.fontFaceProperties": {
                      "default": "warning",
                      "enum": ["ignore", "warning", "error"],
                      "markdownDescription": "`@font-face` rule must define `src` and `font-family` properties.",
                      "type": "string"
                    },
                    "somesass.scss.diagnostics.lint.hexColorLength": {
                      "default": "error",
                      "description": "Hex colors must consist of three or six hex numbers.",
                      "enum": ["ignore", "warning", "error"],
                      "type": "string"
                    },
                    "somesass.scss.diagnostics.lint.argumentsInColorFunction": {
                      "default": "error",
                      "description": "Invalid number of parameters.",
                      "enum": ["ignore", "warning", "error"],
                      "type": "string"
                    },
                    "somesass.scss.diagnostics.lint.unknownProperties": {
                      "default": "warning",
                      "description": "Unknown property.",
                      "enum": ["ignore", "warning", "error"],
                      "type": "string"
                    },
                    "somesass.scss.diagnostics.lint.validProperties": {
                      "default": [],
                      "description": "A list of properties that are not validated against the `unknownProperties` rule.",
                      "items": {
                        "type": "string"
                      },
                      "type": "array",
                      "uniqueItems": true
                    },
                    "somesass.scss.diagnostics.lint.ieHack": {
                      "default": "ignore",
                      "description": "IE hacks are only necessary when supporting IE7 and older.",
                      "enum": ["ignore", "warning", "error"],
                      "type": "string"
                    },
                    "somesass.scss.diagnostics.lint.unknownVendorSpecificProperties": {
                      "default": "ignore",
                      "description": "Unknown vendor specific property.",
                      "enum": ["ignore", "warning", "error"],
                      "type": "string"
                    },
                    "somesass.scss.diagnostics.lint.propertyIgnoredDueToDisplay": {
                      "default": "warning",
                      "enum": ["ignore", "warning", "error"],
                      "markdownDescription": "Property is ignored due to the display. E.g. with `display: inline`, the `width`, `height`, `margin-top`, `margin-bottom`, and `float` properties have no effect.",
                      "type": "string"
                    },
                    "somesass.scss.diagnostics.lint.important": {
                      "default": "ignore",
                      "enum": ["ignore", "warning", "error"],
                      "markdownDescription": "Avoid using `!important`. It is an indication that the specificity of the entire CSS has gotten out of control and needs to be refactored.",
                      "type": "string"
                    },
                    "somesass.scss.diagnostics.lint.float": {
                      "default": "ignore",
                      "enum": ["ignore", "warning", "error"],
                      "markdownDescription": "Avoid using `float`. Floats lead to fragile CSS that is easy to break if one aspect of the layout changes.",
                      "type": "string"
                    },
                    "somesass.scss.diagnostics.lint.idSelector": {
                      "default": "ignore",
                      "description": "Selectors should not contain IDs because these rules are too tightly coupled with the HTML.",
                      "enum": ["ignore", "warning", "error"],
                      "type": "string"
                    },
                    "somesass.scss.diagnostics.lint.unknownAtRules": {
                      "default": "warning",
                      "description": "Unknown at-rule.",
                      "enum": ["ignore", "warning", "error"],
                      "type": "string"
                    },
                    "somesass.scss.foldingRanges.enabled": {
                      "type": "boolean",
                      "default": true,
                      "description": "Enable or disable folding ranges."
                    },
                    "somesass.scss.highlights.enabled": {
                      "type": "boolean",
                      "default": true,
                      "description": "Enable or disable highlights."
